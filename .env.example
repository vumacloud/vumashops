APP_NAME=VumaShops
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_TIMEZONE=Africa/Nairobi
APP_URL=http://localhost

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
APP_MAINTENANCE_STORE=database

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

# Database Configuration
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=vumashops
DB_USERNAME=root
DB_PASSWORD=

# Session Configuration
SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

# Cache Configuration
CACHE_STORE=database
CACHE_PREFIX=vumashops

# Redis (optional)
REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

# Mail - Brevo Configuration
MAIL_MAILER=brevo
MAIL_FROM_ADDRESS="noreply@vumashops.com"
MAIL_FROM_NAME="${APP_NAME}"

BREVO_API_KEY=your-brevo-api-key

# SMS - Africa's Talking Configuration
AFRICASTALKING_USERNAME=sandbox
AFRICASTALKING_API_KEY=your-africastalking-api-key
AFRICASTALKING_FROM=VumaShops

# Paystack Configuration
PAYSTACK_PUBLIC_KEY=your-paystack-public-key
PAYSTACK_SECRET_KEY=your-paystack-secret-key
PAYSTACK_PAYMENT_URL=https://api.paystack.co
PAYSTACK_MERCHANT_EMAIL=merchant@vumashops.com

# Flutterwave Configuration
FLUTTERWAVE_PUBLIC_KEY=your-flutterwave-public-key
FLUTTERWAVE_SECRET_KEY=your-flutterwave-secret-key
FLUTTERWAVE_SECRET_HASH=your-flutterwave-secret-hash
FLUTTERWAVE_ENCRYPTION_KEY=your-flutterwave-encryption-key

# M-Pesa Kenya (Safaricom Daraja) Configuration
MPESA_KENYA_ENV=sandbox
MPESA_KENYA_CONSUMER_KEY=your-consumer-key
MPESA_KENYA_CONSUMER_SECRET=your-consumer-secret
MPESA_KENYA_SHORTCODE=174379
MPESA_KENYA_PASSKEY=your-passkey
MPESA_KENYA_INITIATOR_NAME=testapi
MPESA_KENYA_INITIATOR_PASSWORD=your-initiator-password
MPESA_KENYA_CALLBACK_URL=https://your-domain.com/api/mpesa/callback
MPESA_KENYA_TIMEOUT_URL=https://your-domain.com/api/mpesa/timeout
MPESA_KENYA_RESULT_URL=https://your-domain.com/api/mpesa/result

# M-Pesa Tanzania (Vodacom) Configuration
MPESA_TANZANIA_ENV=sandbox
MPESA_TANZANIA_API_KEY=your-api-key
MPESA_TANZANIA_PUBLIC_KEY=your-public-key
MPESA_TANZANIA_SERVICE_PROVIDER_CODE=000000
MPESA_TANZANIA_CALLBACK_URL=https://your-domain.com/api/mpesa-tz/callback

# MTN Mobile Money Configuration
MTN_MOMO_ENV=sandbox
MTN_MOMO_COLLECTION_SUBSCRIPTION_KEY=your-collection-subscription-key
MTN_MOMO_COLLECTION_API_USER=your-collection-api-user
MTN_MOMO_COLLECTION_API_KEY=your-collection-api-key
MTN_MOMO_DISBURSEMENT_SUBSCRIPTION_KEY=your-disbursement-subscription-key
MTN_MOMO_DISBURSEMENT_API_USER=your-disbursement-api-user
MTN_MOMO_DISBURSEMENT_API_KEY=your-disbursement-api-key
MTN_MOMO_CALLBACK_URL=https://your-domain.com/api/mtn-momo/callback
MTN_MOMO_CURRENCY=EUR
MTN_MOMO_PROVIDER_CALLBACK_HOST=https://your-domain.com

# Airtel Money Configuration
AIRTEL_MONEY_ENV=sandbox
AIRTEL_MONEY_CLIENT_ID=your-client-id
AIRTEL_MONEY_CLIENT_SECRET=your-client-secret
AIRTEL_MONEY_CALLBACK_URL=https://your-domain.com/api/airtel-money/callback
AIRTEL_MONEY_COUNTRY=UG
AIRTEL_MONEY_CURRENCY=UGX

# Tenancy Configuration
TENANCY_CENTRAL_DOMAINS=admin.vumashops.com,vumashops.com

# AWS S3 (Optional for file storage)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"
